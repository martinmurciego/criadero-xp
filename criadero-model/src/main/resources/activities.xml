<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<!-- TIPOS DE EVENTO -->
	<bean id="te_nacimiento" class="ar.uba.fi.criaderoxp.domain.model.TipoEvento">
		<constructor-arg value="nacimiento" />
		<property name="descripcion" value="Nacimiento" />
	</bean>
	<bean id="te_compra" class="ar.uba.fi.criaderoxp.domain.model.TipoEvento">
		<constructor-arg value="compra" />
		<property name="descripcion" value="Compra" />
	</bean>
	<bean id="te_destete" class="ar.uba.fi.criaderoxp.domain.model.TipoEvento">
		<constructor-arg value="destete" />
		<property name="descripcion" value="Destete" />
	</bean>
	<bean id="te_sexado" class="ar.uba.fi.criaderoxp.domain.model.TipoEvento">
		<constructor-arg value="sexado" />
		<property name="descripcion" value="Sexado" />
	</bean>
	<bean id="te_junta" class="ar.uba.fi.criaderoxp.domain.model.TipoEvento">
		<constructor-arg value="junta" />
		<property name="descripcion" value="Junta" />
	</bean>
	<bean id="te_montura" class="ar.uba.fi.criaderoxp.domain.model.TipoEvento">
		<constructor-arg value="montura" />
		<property name="descripcion" value="Montura" />
	</bean>
	<bean id="te_diagnostico" class="ar.uba.fi.criaderoxp.domain.model.TipoEvento">
		<constructor-arg value="diagnostico" />
		<property name="descripcion" value="Diagnóstico de preñez" />
	</bean>
	<bean id="te_parto" class="ar.uba.fi.criaderoxp.domain.model.TipoEvento">
		<constructor-arg value="parto" />
		<property name="descripcion" value="Parto" />
	</bean>
	<bean id="te_desteteCrias" class="ar.uba.fi.criaderoxp.domain.model.TipoEvento">
		<constructor-arg value="desteteCrias" />
		<property name="descripcion" value="Destete de crías" />
	</bean>
	<bean id="te_mudanza" class="ar.uba.fi.criaderoxp.domain.model.TipoEvento">
		<constructor-arg value="mudanza" />
		<property name="descripcion" value="Mudanza" />
	</bean>
	<bean id="te_sacrificio" class="ar.uba.fi.criaderoxp.domain.model.TipoEvento">
		<constructor-arg value="sacrificio" />
		<property name="descripcion" value="Sacrificio" />
	</bean>
	<bean id="te_venta" class="ar.uba.fi.criaderoxp.domain.model.TipoEvento">
		<constructor-arg value="venta" />
		<property name="descripcion" value="Venta" />
	</bean>
	<bean id="te_enfermedad" class="ar.uba.fi.criaderoxp.domain.model.TipoEvento">
		<constructor-arg value="enfermedad" />
		<property name="descripcion" value="Enfermedad" />
	</bean>
	<bean id="te_cura" class="ar.uba.fi.criaderoxp.domain.model.TipoEvento">
		<constructor-arg value="cura" />
		<property name="descripcion" value="Cura" />
	</bean>
	<bean id="te_muerte" class="ar.uba.fi.criaderoxp.domain.model.TipoEvento">
		<constructor-arg value="muerte" />
		<property name="descripcion" value="Muerte" />
	</bean>

	<!-- ESTADOS -->
	<bean id="gazapo" class="ar.uba.fi.criaderoxp.domain.model.Estado">
		<constructor-arg value="gazapo" />
		<property name="descripcion" value="Gazapo" />
		<property name="sano" value="true" />
	</bean>
	<bean id="engorde" class="ar.uba.fi.criaderoxp.domain.model.Estado">
		<constructor-arg value="engorde" />
		<property name="descripcion" value="Engorde" />
		<property name="sano" value="true" />
	</bean>
	<bean id="productor" class="ar.uba.fi.criaderoxp.domain.model.Estado">
		<constructor-arg value="productor" />
		<property name="descripcion" value="Productor" />
		<property name="sano" value="true" />
	</bean>
	<bean id="enEspera" class="ar.uba.fi.criaderoxp.domain.model.Estado">
		<constructor-arg value="enEspera" />
		<property name="descripcion" value="En espera" />
		<property name="sano" value="true" />
	</bean>
	<bean id="juntado" class="ar.uba.fi.criaderoxp.domain.model.Estado">
		<constructor-arg value="juntado" />
		<property name="descripcion" value="Juntado" />
		<property name="sano" value="true" />
	</bean>
	<bean id="montado" class="ar.uba.fi.criaderoxp.domain.model.Estado">
		<constructor-arg value="montado" />
		<property name="descripcion" value="Montado" />
		<property name="sano" value="true" />
	</bean>
	<bean id="preniado" class="ar.uba.fi.criaderoxp.domain.model.Estado">
		<constructor-arg value="preniado" />
		<property name="descripcion" value="Preñado" />
		<property name="sano" value="true" />
	</bean>
	<bean id="amamantando" class="ar.uba.fi.criaderoxp.domain.model.Estado">
		<constructor-arg value="amamantando" />
		<property name="descripcion" value="Amamantando" />
		<property name="sano" value="true" />
	</bean>
	<bean id="sacrificado" class="ar.uba.fi.criaderoxp.domain.model.Estado">
		<constructor-arg value="sacrificado" />
		<property name="descripcion" value="Sacrificado" />
		<property name="sano" value="false" />
	</bean>
	<bean id="vendido" class="ar.uba.fi.criaderoxp.domain.model.Estado">
		<constructor-arg value="vendido" />
		<property name="descripcion" value="Vendido" />
		<property name="sano" value="false" />
	</bean>
	<bean id="enfermo" class="ar.uba.fi.criaderoxp.domain.model.Estado">
		<constructor-arg value="enfermo" />
		<property name="descripcion" value="Enfermo" />
		<property name="sano" value="false" />
	</bean>
	<bean id="muerto" class="ar.uba.fi.criaderoxp.domain.model.Estado">
		<constructor-arg value="muerto" />
		<property name="descripcion" value="Muerto" />
		<property name="sano" value="false" />
	</bean>
	<bean id="nullObjectEstado" class="ar.uba.fi.criaderoxp.domain.model.Estado">
		<constructor-arg value="nullObject" />
		<property name="descripcion" value="Ninguno" />
	</bean>

	<!-- ACTIVIDADES -->
	<bean id="nacimiento" class="ar.uba.fi.criaderoxp.domain.model.Activity">
		<constructor-arg value="nacimiento" />
		<property name="descripcion" value="Nacimiento" />
		<property name="tipoEvento" ref="te_nacimiento" />
		<property name="requisitos">
			<set>
				<ref bean="nullObjectEstado" />
			</set>
		</property>
		<property name="destino" ref="gazapo" />
	</bean>

	<bean id="compra" class="ar.uba.fi.criaderoxp.domain.model.Activity">
		<constructor-arg value="compra" />
		<property name="descripcion" value="Compra" />
		<property name="tipoEvento" ref="te_compra" />
		<property name="requisitos">
			<set>
				<ref bean="nullObjectEstado" />
			</set>
		</property>
		<property name="destino" ref="gazapo" />
	</bean>

	<bean id="destete" class="ar.uba.fi.criaderoxp.domain.model.Activity">
		<constructor-arg value="destete" />
		<property name="descripcion" value="Destete" />
		<property name="tipoEvento" ref="te_destete" />
		<property name="requisitos">
			<set>
				<ref bean="gazapo" />
			</set>
		</property>
		<property name="destino" ref="engorde" />
	</bean>

	<bean id="sexadoProductor" class="ar.uba.fi.criaderoxp.domain.model.Activity">
		<constructor-arg value="sexadoProductor" />
		<property name="descripcion" value="Sexado productor" />
		<property name="tipoEvento" ref="te_sexado" />
		<property name="requisitos">
			<set>
				<ref bean="engorde" />
			</set>
		</property>
		<property name="destino" ref="productor" />
	</bean>

	<bean id="sexadoReproductor" class="ar.uba.fi.criaderoxp.domain.model.Activity">
		<constructor-arg value="sexadoReproductor" />
		<property name="descripcion" value="Sexado reproductor" />
		<property name="tipoEvento" ref="te_sexado" />
		<property name="requisitos">
			<set>
				<ref bean="engorde" />
			</set>
		</property>
		<property name="destino" ref="enEspera" />
	</bean>

	<bean id="junta" class="ar.uba.fi.criaderoxp.domain.model.Activity">
		<constructor-arg value="junta" />
		<property name="descripcion" value="Junta" />
		<property name="tipoEvento" ref="te_junta" />
		<property name="requisitos">
			<set>
				<ref bean="amamantando" />
				<ref bean="enEspera" />
			</set>
		</property>
		<property name="destino" ref="juntado" />
	</bean>

	<bean id="monturaMacho" class="ar.uba.fi.criaderoxp.domain.model.Activity">
		<constructor-arg value="monturaMacho" />
		<property name="descripcion" value="Montura" />
		<property name="tipoEvento" ref="te_montura" />
		<property name="requisitos">
			<set>
				<ref bean="juntado" />
			</set>
		</property>
		<property name="destino" ref="enEspera" />
	</bean>

	<bean id="monturaHembra" class="ar.uba.fi.criaderoxp.domain.model.Activity">
		<constructor-arg value="monturaHembra" />
		<property name="descripcion" value="Montura" />
		<property name="tipoEvento" ref="te_montura" />
		<property name="requisitos">
			<set>
				<ref bean="juntado" />
			</set>
		</property>
		<property name="destino" ref="montado" />
	</bean>

	<bean id="diagnosticoPreniada" class="ar.uba.fi.criaderoxp.domain.model.Activity">
		<constructor-arg value="diagnósticoPreñada" />
		<property name="descripcion" value="Diagnóstico preñada" />
		<property name="tipoEvento" ref="te_diagnostico" />
		<property name="requisitos">
			<set>
				<ref bean="montado" />
			</set>
		</property>
		<property name="destino" ref="preniado" />
	</bean>

	<bean id="diagnosticoNoPreniada" class="ar.uba.fi.criaderoxp.domain.model.Activity">
		<constructor-arg value="diagnósticoNoPreñada" />
		<property name="descripcion" value="Diagnóstico no preñada" />
		<property name="tipoEvento" ref="te_diagnostico" />
		<property name="requisitos">
			<set>
				<ref bean="montado" />
			</set>
		</property>
		<property name="destino" ref="enEspera" />
	</bean>

	<bean id="parto" class="ar.uba.fi.criaderoxp.domain.model.Activity">
		<constructor-arg value="parto" />
		<property name="descripcion" value="Parto" />
		<property name="tipoEvento" ref="te_parto" />
		<property name="requisitos">
			<set>
				<ref bean="preniado" />
			</set>
		</property>
		<property name="destino" ref="amamantando" />
	</bean>

	<bean id="desteteCrias" class="ar.uba.fi.criaderoxp.domain.model.Activity">
		<constructor-arg value="desteteCrías" />
		<property name="descripcion" value="Destete de las crías" />
		<property name="tipoEvento" ref="te_desteteCrias" />
		<property name="requisitos">
			<set>
				<ref bean="amamantando" />
			</set>
		</property>
		<property name="destino" ref="enEspera" />
	</bean>

	<bean id="mudanza" class="ar.uba.fi.criaderoxp.domain.model.Activity">
		<constructor-arg value="mudanza" />
		<property name="descripcion" value="Mudanza" />
		<property name="tipoEvento" ref="te_mudanza" />
		<property name="requisitos">
			<set>
				<ref bean="gazapo" />
				<ref bean="engorde" />
				<ref bean="productor" />
				<ref bean="preniado" />
				<ref bean="enEspera" />
				<ref bean="montado" />
				<ref bean="juntado" />
				<ref bean="amamantando" />
				<ref bean="enfermo" />
			</set>
		</property>
	</bean>

	<bean id="sacrificio" class="ar.uba.fi.criaderoxp.domain.model.Activity">
		<constructor-arg value="sacrificio" />
		<property name="descripcion" value="Sacrificio" />
		<property name="tipoEvento" ref="te_sacrificio" />
		<property name="requisitos">
			<set>
				<ref bean="productor" />
			</set>
		</property>
		<property name="destino" ref="sacrificado" />
	</bean>

	<bean id="venta" class="ar.uba.fi.criaderoxp.domain.model.Activity">
		<constructor-arg value="venta" />
		<property name="descripcion" value="Venta" />
		<property name="tipoEvento" ref="te_venta" />
		<property name="requisitos">
			<set>
				<ref bean="gazapo" />
				<ref bean="engorde" />
				<ref bean="productor" />
				<ref bean="preniado" />
				<ref bean="enEspera" />
				<ref bean="montado" />
				<ref bean="juntado" />
				<ref bean="amamantando" />
				<ref bean="sacrificado" />
			</set>
		</property>
		<property name="destino" ref="vendido" />
	</bean>

	<bean id="enfermedad" class="ar.uba.fi.criaderoxp.domain.model.Activity">
		<constructor-arg value="enfermedad" />
		<property name="descripcion" value="Enfermedad" />
		<property name="tipoEvento" ref="te_enfermedad" />
		<property name="requisitos">
			<set>
				<ref bean="gazapo" />
				<ref bean="engorde" />
				<ref bean="productor" />
				<ref bean="preniado" />
				<ref bean="enEspera" />
				<ref bean="montado" />
				<ref bean="juntado" />
				<ref bean="amamantando" />
			</set>
		</property>
		<property name="destino" ref="enfermo" />
	</bean>

	<bean id="cura" class="ar.uba.fi.criaderoxp.domain.model.Activity">
		<constructor-arg value="cura" />
		<property name="descripcion" value="Cura" />
		<property name="tipoEvento" ref="te_cura" />
		<property name="requisitos">
			<set>
				<ref bean="enfermo" />
			</set>
		</property>
	</bean>

	<bean id="muerte" class="ar.uba.fi.criaderoxp.domain.model.Activity">
		<constructor-arg value="muerte" />
		<property name="descripcion" value="Muerte" />
		<property name="tipoEvento" ref="te_muerte" />
		<property name="requisitos">
			<set>
				<ref bean="gazapo" />
				<ref bean="engorde" />
				<ref bean="productor" />
				<ref bean="preniado" />
				<ref bean="enEspera" />
				<ref bean="montado" />
				<ref bean="juntado" />
				<ref bean="amamantando" />
				<ref bean="enfermo" />
			</set>
		</property>
		<property name="destino" ref="muerto" />
	</bean>

	<bean id="estadosList" class="ar.uba.fi.criaderoxp.BeanList">
		<property name="beans">
			<set>
				<ref bean="gazapo" />
				<ref bean="engorde" />
				<ref bean="productor" />
				<ref bean="enEspera" />
				<ref bean="juntado" />
				<ref bean="montado" />
				<ref bean="preniado" />
				<ref bean="amamantando" />
				<ref bean="sacrificado" />
				<ref bean="vendido" />
				<ref bean="enfermo" />
				<ref bean="muerto" />
				<ref bean="nullObjectEstado" />
			</set>
		</property>
	</bean>

	<bean id="tiposEventoList" class="ar.uba.fi.criaderoxp.BeanList">
		<property name="beans">
			<set>
				<ref bean="te_nacimiento" />
				<ref bean="te_compra" />
				<ref bean="te_destete" />
				<ref bean="te_sexado" />
				<ref bean="te_junta" />
				<ref bean="te_montura" />
				<ref bean="te_diagnostico" />
				<ref bean="te_parto" />
				<ref bean="te_desteteCrias" />
				<ref bean="te_mudanza" />
				<ref bean="te_sacrificio" />
				<ref bean="te_venta" />
				<ref bean="te_enfermedad" />
				<ref bean="te_cura" />
				<ref bean="te_muerte" />
			</set>
		</property>
	</bean>

	<bean id="activitiesList" class="ar.uba.fi.criaderoxp.BeanList">
		<property name="beans">
			<set>
				<ref bean="nacimiento" />
				<ref bean="compra" />
				<ref bean="destete" />
				<ref bean="sexadoProductor" />
				<ref bean="sexadoReproductor" />
				<ref bean="junta" />
				<ref bean="monturaMacho" />
				<ref bean="monturaHembra" />
				<ref bean="diagnosticoPreniada" />
				<ref bean="diagnosticoNoPreniada" />
				<ref bean="parto" />
				<ref bean="desteteCrias" />
				<ref bean="mudanza" />
				<ref bean="sacrificio" />
				<ref bean="venta" />
				<ref bean="enfermedad" />
				<ref bean="cura" />
				<ref bean="muerte" />
			</set>
		</property>
	</bean>

	<bean id="activitiesXMLList" class="ar.uba.fi.criaderoxp.BeanList">
		<property name="beansList">
			<set>
				<ref bean="estadosList" />
				<ref bean="tiposEventoList" />
				<ref bean="activitiesList" />
			</set>
		</property>
	</bean>
</beans>