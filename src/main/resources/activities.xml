<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<!-- TIPOS DE EVENTO -->
	<bean id="te_nacimiento" class="main.java.ar.uba.fi.criaderoxp.domain.model.TipoEvento">
		<constructor-arg value="nacimiento" />
	</bean>
	<bean id="te_destete" class="main.java.ar.uba.fi.criaderoxp.domain.model.TipoEvento">
		<constructor-arg value="destete" />
	</bean>
	<bean id="te_sexado" class="main.java.ar.uba.fi.criaderoxp.domain.model.TipoEvento">
		<constructor-arg value="sexado" />
	</bean>
	<bean id="te_junta" class="main.java.ar.uba.fi.criaderoxp.domain.model.TipoEvento">
		<constructor-arg value="junta" />
	</bean>
	<bean id="te_montura" class="main.java.ar.uba.fi.criaderoxp.domain.model.TipoEvento">
		<constructor-arg value="montura" />
	</bean>
	<bean id="te_diagnostico" class="main.java.ar.uba.fi.criaderoxp.domain.model.TipoEvento">
		<constructor-arg value="diagnostico" />
	</bean>
	<bean id="te_parto" class="main.java.ar.uba.fi.criaderoxp.domain.model.TipoEvento">
		<constructor-arg value="parto" />
	</bean>
	<bean id="te_desteteCrias" class="main.java.ar.uba.fi.criaderoxp.domain.model.TipoEvento">
		<constructor-arg value="desteteCrias" />
	</bean>
	<bean id="te_mudanza" class="main.java.ar.uba.fi.criaderoxp.domain.model.TipoEvento">
		<constructor-arg value="mudanza" />
	</bean>
	<bean id="te_sacrificio" class="main.java.ar.uba.fi.criaderoxp.domain.model.TipoEvento">
		<constructor-arg value="sacrificio" />
	</bean>
	<bean id="te_venta" class="main.java.ar.uba.fi.criaderoxp.domain.model.TipoEvento">
		<constructor-arg value="venta" />
	</bean>
	<bean id="te_enfermedad" class="main.java.ar.uba.fi.criaderoxp.domain.model.TipoEvento">
		<constructor-arg value="enfermedad" />
	</bean>
	<bean id="te_cura" class="main.java.ar.uba.fi.criaderoxp.domain.model.TipoEvento">
		<constructor-arg value="cura" />
	</bean>
	<bean id="te_muerte" class="main.java.ar.uba.fi.criaderoxp.domain.model.TipoEvento">
		<constructor-arg value="muerte" />
	</bean>

	<!-- ESTADOS -->
	<bean id="gazapo" class="main.java.ar.uba.fi.criaderoxp.domain.model.Estado">
		<constructor-arg value="Gazapo" />
	</bean>
	<bean id="engorde" class="main.java.ar.uba.fi.criaderoxp.domain.model.Estado">
		<constructor-arg value="Engorde" />
	</bean>
	<bean id="productor" class="main.java.ar.uba.fi.criaderoxp.domain.model.Estado">
		<constructor-arg value="Productor" />
	</bean>
	<bean id="enEspera" class="main.java.ar.uba.fi.criaderoxp.domain.model.Estado">
		<constructor-arg value="En espera" />
	</bean>
	<bean id="juntado" class="main.java.ar.uba.fi.criaderoxp.domain.model.Estado">
		<constructor-arg value="Juntado" />
	</bean>
	<bean id="montado" class="main.java.ar.uba.fi.criaderoxp.domain.model.Estado">
		<constructor-arg value="Montado" />
	</bean>
	<bean id="preniado" class="main.java.ar.uba.fi.criaderoxp.domain.model.Estado">
		<constructor-arg value="Preniado" />
	</bean>
	<bean id="amamantando" class="main.java.ar.uba.fi.criaderoxp.domain.model.Estado">
		<constructor-arg value="Amamantando" />
	</bean>
	<bean id="sacrificado" class="main.java.ar.uba.fi.criaderoxp.domain.model.Estado">
		<constructor-arg value="Sacrificado" />
	</bean>
	<bean id="vendido" class="main.java.ar.uba.fi.criaderoxp.domain.model.Estado">
		<constructor-arg value="Vendido" />
	</bean>
	<bean id="enfermo" class="main.java.ar.uba.fi.criaderoxp.domain.model.Estado">
		<constructor-arg value="Enfermo" />
	</bean>
	<bean id="muerto" class="main.java.ar.uba.fi.criaderoxp.domain.model.Estado">
		<constructor-arg value="Muerto" />
	</bean>
	<bean id="nullObjectEstado" class="main.java.ar.uba.fi.criaderoxp.domain.model.Estado">
		<constructor-arg value="Null object" />
	</bean>

	<!-- ACTIVIDADES -->
	<bean id="nacimiento" class="main.java.ar.uba.fi.criaderoxp.domain.model.Activity">
		<constructor-arg value="Nacimiento" />
		<property name="tipoEvento" ref="te_nacimiento" />
		<property name="requisitos">
			<set>
				<ref bean="nullObjectEstado" />
			</set>
		</property>
		<property name="destino" ref="gazapo" />
	</bean>
	<bean id="destete" class="main.java.ar.uba.fi.criaderoxp.domain.model.Activity">
		<constructor-arg value="Destete" />
		<property name="tipoEvento" ref="te_destete" />
		<property name="requisitos">
			<set>
				<ref bean="gazapo" />
			</set>
		</property>
		<property name="destino" ref="engorde" />
	</bean>

	<bean id="sexadoProductor" class="main.java.ar.uba.fi.criaderoxp.domain.model.Activity">
		<constructor-arg value="Sexado productor" />
		<property name="tipoEvento" ref="te_sexado" />
		<property name="requisitos">
			<set>
				<ref bean="engorde" />
			</set>
		</property>
		<property name="destino" ref="productor" />
	</bean>

	<bean id="sexadoReproductor" class="main.java.ar.uba.fi.criaderoxp.domain.model.Activity">
		<constructor-arg value="Sexado reproductor" />
		<property name="tipoEvento" ref="te_sexado" />
		<property name="requisitos">
			<set>
				<ref bean="engorde" />
			</set>
		</property>
		<property name="destino" ref="enEspera" />
	</bean>

	<bean id="junta" class="main.java.ar.uba.fi.criaderoxp.domain.model.Activity">
		<constructor-arg value="Junta" />
		<property name="tipoEvento" ref="te_junta" />
		<property name="requisitos">
			<set>
				<ref bean="amamantando" />
				<ref bean="enEspera" />
			</set>
		</property>
		<property name="destino" ref="juntado" />
	</bean>

	<bean id="monturaMacho" class="main.java.ar.uba.fi.criaderoxp.domain.model.Activity">
		<constructor-arg value="Montura macho" />
		<property name="tipoEvento" ref="te_montura" />
		<property name="requisitos">
			<set>
				<ref bean="juntado" />
			</set>
		</property>
		<property name="destino" ref="enEspera" />
	</bean>

	<bean id="monturaHembra" class="main.java.ar.uba.fi.criaderoxp.domain.model.Activity">
		<constructor-arg value="Montura hembra" />
		<property name="tipoEvento" ref="te_montura" />
		<property name="requisitos">
			<set>
				<ref bean="juntado" />
			</set>
		</property>
		<property name="destino" ref="montado" />
	</bean>

	<bean id="diagnosticoPreniada" class="main.java.ar.uba.fi.criaderoxp.domain.model.Activity">
		<constructor-arg value="Diagnóstico preñada" />
		<property name="tipoEvento" ref="te_diagnostico" />
		<property name="requisitos">
			<set>
				<ref bean="montado" />
			</set>
		</property>
		<property name="destino" ref="preniado" />
	</bean>

	<bean id="diagnosticoNoPreniada" class="main.java.ar.uba.fi.criaderoxp.domain.model.Activity">
		<constructor-arg value="Diagnóstico no preñada" />
		<property name="tipoEvento" ref="te_diagnostico" />
		<property name="requisitos">
			<set>
				<ref bean="montado" />
			</set>
		</property>
		<property name="destino" ref="enEspera" />
	</bean>

	<bean id="parto" class="main.java.ar.uba.fi.criaderoxp.domain.model.Activity">
		<constructor-arg value="Parto" />
		<property name="tipoEvento" ref="te_parto" />
		<property name="requisitos">
			<set>
				<ref bean="preniado" />
			</set>
		</property>
		<property name="destino" ref="amamantando" />
	</bean>

	<bean id="desteteCrias" class="main.java.ar.uba.fi.criaderoxp.domain.model.Activity">
		<constructor-arg value="Destete crías" />
		<property name="tipoEvento" ref="te_desteteCrias" />
		<property name="requisitos">
			<set>
				<ref bean="amamantando" />
			</set>
		</property>
		<property name="destino" ref="enEspera" />
	</bean>

	<bean id="mudanza" class="main.java.ar.uba.fi.criaderoxp.domain.model.Activity">
		<constructor-arg value="Mudanza" />
		<property name="tipoEvento" ref="te_mudanza" />
		<property name="requisitos">
			<set>
				<ref bean="gazapo" />
				<ref bean="engorde" />
				<ref bean="productor" />
				<ref bean="preniado" />
				<ref bean="enEspera" />
				<ref bean="montado" />
				<ref bean="juntado" />
				<ref bean="amamantando" />
				<ref bean="enfermo" />
			</set>
		</property>
	</bean>

	<bean id="sacrificio" class="main.java.ar.uba.fi.criaderoxp.domain.model.Activity">
		<constructor-arg value="Sacrificio" />
		<property name="tipoEvento" ref="te_sacrificio" />
		<property name="requisitos">
			<set>
				<ref bean="productor" />
			</set>
		</property>
		<property name="destino" ref="sacrificado" />
	</bean>

	<bean id="venta" class="main.java.ar.uba.fi.criaderoxp.domain.model.Activity">
		<constructor-arg value="Venta" />
		<property name="tipoEvento" ref="te_venta" />
		<property name="requisitos">
			<set>
				<ref bean="gazapo" />
				<ref bean="engorde" />
				<ref bean="productor" />
				<ref bean="preniado" />
				<ref bean="enEspera" />
				<ref bean="montado" />
				<ref bean="juntado" />
				<ref bean="amamantando" />
				<ref bean="sacrificado" />
			</set>
		</property>
		<property name="destino" ref="vendido" />
	</bean>

	<bean id="enfermedad" class="main.java.ar.uba.fi.criaderoxp.domain.model.Activity">
		<constructor-arg value="Enfermedad" />
		<property name="tipoEvento" ref="te_enfermedad" />
		<property name="requisitos">
			<set>
				<ref bean="gazapo" />
				<ref bean="engorde" />
				<ref bean="productor" />
				<ref bean="preniado" />
				<ref bean="enEspera" />
				<ref bean="montado" />
				<ref bean="juntado" />
				<ref bean="amamantando" />
			</set>
		</property>
		<property name="destino" ref="enfermo" />
	</bean>

	<bean id="cura" class="main.java.ar.uba.fi.criaderoxp.domain.model.Activity">
		<constructor-arg value="Cura" />
		<property name="tipoEvento" ref="te_cura" />
		<property name="requisitos">
			<set>
				<ref bean="enfermo" />
			</set>
		</property>
	</bean>

	<bean id="muerte" class="main.java.ar.uba.fi.criaderoxp.domain.model.Activity">
		<constructor-arg value="Muerte" />
		<property name="tipoEvento" ref="te_muerte" />
		<property name="requisitos">
			<set>
				<ref bean="gazapo" />
				<ref bean="engorde" />
				<ref bean="productor" />
				<ref bean="preniado" />
				<ref bean="enEspera" />
				<ref bean="montado" />
				<ref bean="juntado" />
				<ref bean="amamantando" />
				<ref bean="enfermo" />
			</set>
		</property>
		<property name="destino" ref="muerto" />
	</bean>
</beans>